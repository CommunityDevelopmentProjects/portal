{% extends "account/base.html" %}
{% load widget_tweaks %}
{% load static %}
{% block content%}
        <div class="container-fluid">

                        <!-- start page title -->
                        <div class="row">
                            <div class="col-12">
                                <div class="page-title-box">
                                    <div class="page-title-right">
                                        <ol class="breadcrumb m-0">
                                            
                                            <li class="breadcrumb-item active">My Profile </li>
                                        </ol>
                                    </div>
                                    <h4 class="page-title">My Profile</h4>
                                </div>
                            </div>
                        </div>     
                        <!-- end page title --> 

                        <div class="row">
                            <div class="col-xl-4 col-lg-5">
                                <div class="card text-center">
                                    <div class="card-body">
                                        <img src={% if active_user.profile_image%} "{{active_user.profile_image.url}}"{%else%}"{% static 'account/images/profile.png' %}"{%endif%} class="rounded-circle w-50 img-thumbnail" alt="profile-image">
										{% if request.user.is_authenticated %}
                                        <h4 class="mb-0 mt-2">{{request.user.user_name}}</h4>
										{% endif %}
                                        <p class="text-muted font-14">{{request.user.phone_number}}</p>
<!--
                                        <button type="button" class="btn btn-success btn-sm mb-2">Follow</button>
                                        <button type="button" class="btn btn-danger btn-sm mb-2">Message</button>
-->
                                        <div class="text-start mt-3">
                                            <h4 class="font-13 text-uppercase">About Me :</h4>
											
                                            <p class="text-muted font-13 mb-3">
                                                
													{% if active_user.about_me%}
														{{active_user.about_me}}
													{%else%}
													Tell us a little about you
													{% endif %}
												
                                            </p>
											{% if active_user.full_name%}
                                            <p class="text-muted mb-2 font-13"><strong>Full Name :</strong> <span class="ms-2">
											{{active_user.full_name}}</span></p>
											{% endif%}
											
											{% if active_user.mobile%}
                                            <p class="text-muted mb-2 font-13"><strong>Mobile :</strong><span class="ms-2">
											{{active_user.mobile}}</span></p>
											{%endif%}
											
											{% if active_user.phone%}
                                            <p class="text-muted mb-2 font-13"><strong>Phone :</strong><span class="ms-2">
											{{active_user.phone}}</span></p>
											{%endif%}
											
											{% if active_user.email%}
                                            <p class="text-muted mb-2 font-13"><strong>Email :</strong> <span class="ms-2 ">
											{{active_user.email}}</span></p>
											{% endif %}
											
											{% if active_user.address%}
                                            <p class="text-muted mb-1 font-13"><strong>Location :</strong> <span class="ms-2">
											{{active_user.address}}</span></p>
											{% endif %}
											
											{% if active_user.languages%}
                                            <p class="text-muted mb-1 font-13"><strong>Languages :</strong> <span class="ms-2">
											{{active_user.languages}}</span></p>
											{% endif %}
											
											{% if active_user.best_skill%}
                                            <p class="text-muted mb-1 font-13"><strong>Best Skill:</strong> <span class="ms-2">
											{{active_user.best_skill}}</span></p>
											{% endif %}
											
											{% if active_user.employment%}
                                            <p class="text-muted mb-1 font-13"><strong>Empolyement :</strong> <span class="ms-2">
											{{active_user.employment}}</span></p>
											{% endif %}
											
											{% if active_user.status%}
                                            <p class="text-muted mb-1 font-13"><strong>Status :</strong> <span class="ms-2">
											{{active_user.status}}</span></p>
											{% endif %}
											
										</div>

                                        <ul class="social-list list-inline mt-3 mb-0">
										{% if active_user.facebook%}
                                            
                                            <li class="list-inline-item">
                                                <a href="{{active_user.facebook}}" class="social-list-item border-primary text-primary"><i class="mdi mdi-facebook"></i></a>
                                            </li>
										{%endif%}
										{% if active_user.instagram%}
                                            <li class="list-inline-item">
                                                <a href="{{active_user.instagram}}" class="social-list-item border-danger text-danger"><i class="mdi mdi-instagram"></i></a>
                                            </li>
										{%endif%}
										{% if active_user.twitter%}
                                            <li class="list-inline-item">
